{
  "name": "atomclinic",
  "version": "1.0.0",
  "description": "clinic app written in electron",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "cleanup": "rm -rf atomclinic-*-x64",
    "pack:unix": "electron-packager . atomclinic --platform=darwin,linux --arch=x64 --version=0.33.0 --ignore=electron-*,.git* --asar --icon=./icons/noun_38389_cc.icns",
    "pack:win": "electron-packager . atomclinic --platform=win32 --arch=x64 --version=0.33.0 --ignore=electron-*,.git* --asar --icon=./icons/noun_38389_cc.ico",
    "rebuild": "electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlhkh/atomclinic.git"
  },
  "author": "Kha Nguyen <nlhkha@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlhkh/atomclinic/issues"
  },
  "homepage": "https://github.com/nlhkh/atomclinic#readme",
  "dependencies": {
    "alt": "^0.17.3",
    "babel": "^5.8.23",
    "nedb": "^1.2.0",
    "numeral": "^1.5.3",
    "react": "^0.13.3",
    "react-portal": "^0.7.0",
    "react-router": "^0.13.3",
    "skeleton-css": "^2.0.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.33.0",
    "electron-rebuild": "^1.0.0"
  }
}
